<div class="row ">
    <div class="col-md-2">
        <%= render 'tradeinfo_menu' %>
    </div>
    <div class="col-md-10">
        <div class="panel panel-default">

            <div class="panel-heading">
                Eligibility
            </div>

            <div class="panel-body">
                <div class="container">
                    <%= form_for @tradeinfo do |f| %>


                        <div id="DIV_226">

                            <div class="row">
                                    <label class="control-label"> Product Information</label>


                                    <div class="row">
                                        <br>
                                        <div class="col-md-6">
                                            <p> What products do you export </p>
                                            <div> <%= f.text_field :goods, class: "form-control" %> </div>
                                        </div>

                                    </div>
                                    <br><br>

                                    <div class="row">
                                        <div class="col-md-6">
                                            <p> What is the category of products exported? </p>

                                            <%= f.select :category, [["Aeronautical Engineering Products", "Aeronautical Engineering Products"], ["Agro Products", "Agro Products"], ["Bed Linen", "Bed Linen"],
                                                                     ["Cashews", "Cashews"], ["Chemicals", "Chemicals"], ["Coffee and Coffee Products", "Coffee and Coffee Products"],
                                                                     ["Cosmetics", "Cosmetics"], ["Cotton Yarn", "Cotton Yarn"],
                                                                     ["Cotton/Jute Bags", "Cotton/Jute Bags"], ["Engineering Products", "Engineering Products"],
                                                                     ["FMCGs", "FMCGs"], ["Frozen Meats", "Frozen Meats"],
                                                                     ["Grains", "Grains"], ["Pulses", "Pulses"], ["Granites", "Granites"], ["Herbs and Spices", "Herbs and Spices"]],
                                                         id: "B_168", prompt: "Select..." %>

                                        </div>
                                    </div>

                                <br>
                                <hr align="middle" background-color="#322d59" width="100%">

                                <div class="form-group">
                                    <label class="control-label"> Exporter Information </label>

                                    <div class="row">
                                        <br>
                                        <div class="col-md-4">
                                            <p> Company Name </p>
                                            <div> <%= f.text_field :companyName, class: "form-control" %> </div>
                                        </div>
                                    </div>


                                    <div class="row">
                                        <br>
                                        <div class="col-md-4">
                                            <p> Phone </p>
                                            <div> <%= f.text_field :companyPhone, class: "form-control" %> </div>
                                        </div>
                                        <div class="col-md-4 col-md-offset-2">
                                            <p> Tax Number </p>
                                            <div> <%= f.text_field :companyEmail, class: "form-control"%> </div>
                                        </div>
                                    </div>
                                </div>

                                <hr align="middle" background-color="#322d59" width="100%">


                                <div class="form-group">
                                    <label class="control-label"> Importer Information</label>

                                    <div class="row">
                                        <br>
                                        <div class="col-md-4">
                                            <p> Company Name </p>
                                            <div> <%= f.text_field :importerName, class: "form-control" %> </div>
                                        </div>
                                        <div class="col-md-5 col-md-offset-2">
                                            <p> Email </p>
                                            <div> <%= f.text_field :importerEmail, class: "form-control" %> </div>
                                        </div>
                                    </div>


                                    <div class="row">
                                        <br>
                                        <div class="col-md-4">
                                            <p> Phone </p>
                                            <div> <%= f.text_field :companyEmail, class: "form-control" %> </div>
                                        </div>
                                        <div class="col-md-5 col-md-offset-2">
                                            <p> Tax Number </p>
                                            <div> <%= f.text_field :companyEmail, class: "form-control" %> </div>
                                        </div>
                                    </div>

                                </div>

                                <br>
                                <br>

                                <div class="text-center">
                                    <%= f.submit "Save", class: "btn btn-form" %>
                                </div>


                                <div id="DIV_295">
                                    <%= button_to "Save", new_tradeinfo_buyer_path(@tradeinfo), id:"A_296", method: :get %>
                                </div>


                            </div>
                        </div>

                    <% end %>
                </div>
            </div>
        </div>
    </div>
</div>