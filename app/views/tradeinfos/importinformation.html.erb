<section id="SECTION_85">
    <div id="DIV_85">
        <%= form_for @tradeinfo do |f| %>
            <div id="DIV_91">

                <div id="DIV_112">
                    <h1 id="H1_224">
                        Importer Information
                    </h1>

                    <div id="DIV_228">
                        <div class="form-group">

                            <div class="row">
                                <div class="col-md-6 col-md-offset-3">
                                    <label class="control-label"> Company Name </label>
                                    <div> <%= f.text_field :importerName, class: "form-control", required: true %> </div>
                                </div>
                            </div>

                            <br><br>

                            <div class="row">
                                <div class="col-md-6 col-md-offset-3">
                                    <label class="control-label"> Tax Number </label>
                                    <div> <%= f.text_field :importerTaxId, class: "form-control", required: true %> </div>
                                </div>
                            </div>

                            <br><br>

                            <div class="row">
                                <div class="col-md-6 col-md-offset-3">
                                    <label class="control-label"> Email </label>
                                    <div> <%= f.text_field :importerEmail, class: "form-control", required: true %> </div>
                                </div>
                            </div>
                            <br>
                        </div>

                    </div>

                    <div id="DIV_296">
                        <a href="#details" id="A_296">Back</a>
                        <button type="submit" name="application[stage]" value="-1" id="BUTTON_297">
                            Submit
                        </button>
                    </div>
                </div>

            </div>
        <% end %>
    </div>
</section>
<style>

    body {
        background-image: url(<%= asset_path 'banner4.jpg' %>);
    }

</style>