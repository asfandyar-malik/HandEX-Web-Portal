<div id="loader" style="display:none; padding-top: 15%; text-align: center;">
    <img id="myImage" , src="<%= asset_path('bx_loader.gif') %>"></div><br>


<section id="SECTION_84">
    <div id="DIV_85">
        <div id="DIV_111">
            <h1 id="H1_224">
                <img class="responsive-img" id="IMG_214" src="<%= asset_path('EH1.png') %>">
            </h1>
        </div>

        <div id="DIV_88">
            <div id="DIV_89">
                <div id="DIV_90">
                    Money Applied for: <b> <%= @tradeinfo.financial.total_financing_required %> </b> <br>
                    Duration: <b><%= @tradeinfo.financial.time_duration %>  </b> <br>
                    Insurance Premium: <b><%= @mainInsurancePremium %>  </b> <br>
                    Foreign Currency Coverage: <b><%= @foreignCurrencyCoverage %>  </b> <br>
                    Reducing Deductible: <b><%= @reducingDeductible %>  </b> <br>
                    Execution Amount: <b><%= @executionAmountOne %>  </b> <br>
                    Final Sum: <b><%= @finalSum %>  </b> <br>

                </div>
            </div>
        </div>
    </div>

    <div id="DIV_88">

        <% if @tradeinfo.bankaccount %>
            <%= button_to "Submit Bank Account Information",  tradeinfo_bankaccounts_index_path(@tradeinfo), method: :get, id:"BUTTON_298" %>
        <% else %>
            <%= button_to "Submit Bank Account Information", new_tradeinfo_bankaccount_path(@tradeinfo), method: :get, id:"BUTTON_298" %>
        <% end %>

        <br>
    </div>

</section>

<footer id="FOOTER_409">
</footer>

<script>
    $(document).ready(function () {

        $('.datepicker').pickadate({
            selectMonths: true, // Creates a dropdown to control month
            selectYears: 2 // Creates a dropdown of 15 years to control year
        });
    });

    (function () {
        var loader = document.getElementById("loader");
        var section = document.getElementById("SECTION_84");
        var footer = document.getElementById("FOOTER_409");

        show = function () {
            loader.style.display = "block";
            section.style.display = "none";
            footer.style.display = "none";
            setTimeout(hide, 000);
        },

            hide = function () {
                loader.style.display = "none";
                section.style.display = "block";
                footer.style.display = "block";
            };
        show();
    })();


</script>