$('#photos').html("<%= j render 'photos_list' %>")

<% if @document.photos.blank? %>
$('#photo_check').hide();
<% end %>


<% if !@document.document_type.blank? && !@document.year.blank? %>
$('#publish_button').attr('disabled', false);
<% else %>
$('#publish_button').attr('disabled', true);
<% end %>