<!-- step_5 start -->
<div class="step_2 tab" id="step_2">

    <!--   Form Navigation Section   -->
    <div class="row form-nav-top">
        <div class="col s12 m4">
            <div class="form-nav form-nav-percent">
                <p><span> 80% Complete</span></p>
            </div>
        </div>
        <div class="col s12 m4">
            <div class="form-nav">
                <h2>Documents</h2>
            </div>
        </div>
        <div class="col s12 m4">
            <div class="form-nav right">
                <!--                <input class="btn handex-btn z-depth-0" type="submit" name="draft" value = "<%#= t("save_draft") %>">-->
                <%#= form.submit class: 'btn handex-btn z-depth-0', value: t('application_submit') %>

                <input class="btn handex-btn z-depth-0" type="submit" name="draft" value="Save Draft">
                <%= form.submit class: 'btn handex-btn z-depth-0', value: "Submit Application" %>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col s12">

            <div class="form-content">
                <div class="input-field col s12 m12">
                    <p> We need the documents below to arrange the best payment terms for you. In Other, you can provide any supporting documents that underscore the financial strength of your
                        company.</p>
                </div>

                <div class="input-field col s12 m12 ">
                    <%= form.label :annual_financial_statement_importer_last, "Annual Report (2018)" %>
                    <div class="file-field input-field">
                        <div class="btn handex-btn z-depth-0">
                            <span><%= t('browse') %></span>
                            <%= form.file_field :annual_financial_statement_importer_last %>
                        </div>

                        <div class="file-path-wrapper">
                            <input class="file-path validate" type="text"/>
                        </div>
                    </div>
                </div>

                <div class="input-field col s12 m12 ">
                    <%= form.label :annual_financial_statement_importer_second_last, "Annual Report (2017)" %>
                    <div class="file-field input-field">
                        <div class="btn handex-btn z-depth-0">
                            <span><%= t('browse') %></span>
                            <%= form.file_field :annual_financial_statement_importer_second_last %>
                        </div>

                        <div class="file-path-wrapper">
                            <input class="file-path validate" type="text"/>
                        </div>
                    </div>
                </div>

                <div class="input-field col s12 m12 ">
                    <%= form.label :credit_bureaus_importer, "Credit Agency Report (if available)" %>
                    <div class="file-field input-field">
                        <div class="btn handex-btn z-depth-0">
                            <span><%= t('browse') %></span>
                            <%= form.file_field :credit_bureaus_importer %>
                        </div>

                        <div class="file-path-wrapper">
                            <input class="file-path validate" type="text"/>
                        </div>
                    </div>
                </div>

                <div class="input-field col s12 m12 ">
                    <%= form.label :other_document, "Other" %>
                    <div class="file-field input-field">
                        <div class="btn handex-btn z-depth-0">
                            <span><%= t('browse') %></span>
                            <%= form.file_field :other_document %>
                        </div>

                        <div class="file-path-wrapper">
                            <input class="file-path validate" type="text"/>
                        </div>
                    </div>
                </div>

                <div class="background_toggle prevent_exporter_from_viewing_documents <%= (@importer_information.prevent_exporter_from_viewing_documents.present? && @importer_information.prevent_exporter_from_viewing_documents) ? 'dialog_background' : '' %>">

                    <div class="input-field col s12 m12">
                        <%= form.label :prevent_exporter_from_viewing_documents, "Do you want us to avoid that your supplier sees your annual report and other documents provided here" %>
                        <br><br>

                        <label onclick="preventExporterFromViewingDocs()">
                            <span class="yes yes-no-btns prevent_exporter_from_viewing_documents <%= 'active-btn' if (@importer_information.prevent_exporter_from_viewing_documents) %>">
                                <%= check_box_tag "importer_information[prevent_exporter_from_viewing_documents]", '1' %>
                                Yes
                            </span>
                        </label>

                        <label onclick="dontPreventExporterFromViewingDocs()">
                            <span class="no yes-no-btns prevent_exporter_from_viewing_documents <%= 'active-btn' if (!@importer_information.prevent_exporter_from_viewing_documents.nil? && !@importer_information.prevent_exporter_from_viewing_documents) %>">
                                <%= check_box_tag "importer_information[prevent_exporter_from_viewing_documents]", '0' %>
                                No
                            </span>
                        </label>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>