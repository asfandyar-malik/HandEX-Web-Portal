<% if application.application_type == "export_application" || application.application_type == "insurance" %>
    
    <div class="col s12 m3">
        <div class="card">
            <div class="card-title approved_financing_application white-text valign-wrapper">
                <div class="valign-wrapper">
                    <i class="custom_icon check-icon"></i>
                    <div class="card-header">
                        Antrag genehmigt
                    </div>
                </div>
            </div>
            
            <div class="card-content">
                
<!--                <div class="valign-wrapper">-->
<!--                    <div class="col m2">-->
<!--                        <i class="custom_icon box-icon"></i>-->
<!--                    </div>-->
<!--                    <div class="col m10">-->
<!--                        <div class="summary"> <%#= t('application_type') %></div>-->
<!--                        <p class="bold-font"><%#= application.application_type %></p>-->
<!--                    </div>-->
<!--                </div>-->
                
                <div class="valign-wrapper">
                    <div class="col m2">
                        <i class="custom_icon import-icon"></i>
                    </div>
                    <div class="col m10">
                        <div class="output_importer"> <%= t('importer') %></div>
                        <p><b><%= (application.importer_company_name == '') ? '-' : application.importer_company_name %></b></p>
                    </div>
                </div>
    
                <div class="valign-wrapper">
                    <div class="col m2">
                        <i class="custom_icon import-icon"></i>
                    </div>
                    <div class="col m10">
                        <div class="summary"> <%= t('destination_country') %></div>
                        <p><b><%= (application.importer_country == 'Bitte ausw채hlen') ? '-' : application.importer_country %></b></p>
                    </div>
                </div>
                
                <div class="valign-wrapper">
                    <div class="col m2">
                        <i class="custom_icon box-icon"></i>
                    </div>
                    <div class="col m10">
                        <div class="summary"> <%= t('product') %> </div>
                        <p><b><%= (application.exported_goods_type_two == 'Bitte w채hlen Sie eine Warenkategorie, die zu Ihrem Produkt passt') ? '-' : application.exported_goods_type_two %></b></p>
                    </div>
                </div>
                
                <div class="valign-wrapper">
                    <div class="col m2">
                        <i class="custom_icon money-icon"></i>
                    </div>
                    <div class="col m10">
                        <div class="summary"> <%= t('contract_value') %></div>
                        <p><b><%= (application.contract_value == '') ? '-' : application.contract_value  %></b></p>
                    </div>
                </div>
                
                <div class="valign-wrapper">
                    <div class="col m2">
                        <i class="custom_icon send-icon"></i>
                    </div>
                    <div class="col m10">
                        <div class="summary"> <%= t('application_sent_at') %></div>
                        <p><b><%= (application.updated_at == '') ? '-' : application.updated_at.strftime('%d %b %Y') %></b></p>
                    </div>
                </div>
                
                <% if application.application_type == "export_application" %>
                    <div class="card-action center">
                        <%= link_to t('see_summary_of_application'), controller: "export_applications", action: "show", id: application %>
                    </div>
                <% end %>
                
                <% if application.application_type == "insurance" %>
                    <div class="card-action">
                        <%= link_to t('see_summary_of_application'), controller: "insurances", action: "show", id: application %>
                    </div>
                <% end %>
                
<!--                <div class="center">-->
                    <%#= link_to 'Deckungzusage <i class="inline-icon material-icons">cloud_download</i>'.html_safe, "/sample.pdf", {target: '_blank', class: 'btn btn-primary btn-xs', download: "Deckungzusage "} %>
<!--                </div>-->
    
                <% if application.is_docusign_step %>
                    
                    <div class="center">
                        <br>
                        <a href= "<%= new_docusign_path %>" class="smooth"><%= image_tag "docusign.jpg", style: 'height:4em;width:auto;' %></a>
                    </div>
                
                <% else %>
                    
                    <% if application.webid_identification %>
                        <div class="center">
                            <br>
                            <div> Wir k체mmern uns um Ihren Forfaitierungsvertrag, bitte schauen Sie nochmal hier drin in 24 Stunden
                            </div>
                        </div>
                    <% else %>
                        <div class="center">
                            <br>
                            <%= link_to 'Weiter zu WebID', new_webid_path %>
                        </div>
                    <% end %>
    
                <% end %>
                
            </div>
        </div>
    </div>

