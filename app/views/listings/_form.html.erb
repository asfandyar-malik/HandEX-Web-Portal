<div class="container">
    <div class="row">
        <div class="col-sm-12">

            <div class="wizard-container">
                <div class="card wizard-card" data-color="blue" id="wizard">

                    <%= form_with(model: listing, local: true) do |form| %>

                        <% if listing.errors.any? %>
                            <div id="error_explanation">
                                <h2><%= pluralize(listing.errors.count, "error") %> prohibited this listing from
                                    being saved:</h2>

                                <ul>
                                    <% listing.errors.full_messages.each do |message| %>
                                        <li><%= message %></li>
                                    <% end %>
                                </ul>
                            </div>
                        <% end %>

                        <div class="field">
                        </div>

                        <div class="wizard-header">
                            <h3 class="wizard-title">Versichern Sie Ihren Export</h3>
                            <h5 align="center">In Kooperation mit Euler Hermes</h5>
                            <br><br>
                        </div>
                        <div class="wizard-navigation">

                            <div class="progress-with-circle">
                                <div class="progress-bar" role="progressbar" aria-valuenow="1" aria-valuemin="1" aria-valuemax="4" style="width: 15%;"></div>
                            </div>

                            <ul>
                                <li>
                                    <a href="#contactinformation" data-toggle="tab">
                                        <div class="icon-circle">
                                            <i class="ti-clipboard"></i>
                                        </div>
                                        Kontaktinformation
                                    </a>
                                </li>
                                <li>
                                    <a href="#location" data-toggle="tab">
                                        <div class="icon-circle">
                                            <i class="ti-map"></i>
                                        </div>
                                        Das Wichtigste Zuerst
                                    </a>
                                </li>
                                <li>
                                    <a href="#type" data-toggle="tab">
                                        <div class="icon-circle">
                                            <i class="ti-direction-alt"></i>
                                        </div>
                                        Ihre Risiken
                                    </a>
                                </li>
                                <li>
                                    <a href="#facilities" data-toggle="tab">
                                        <div class="icon-circle">
                                            <i class="ti-panel"></i>
                                        </div>
                                        Ihr Geschäft
                                    </a>
                                </li>
                                <li>
                                    <a href="#description" data-toggle="tab">
                                        <div class="icon-circle">
                                            <i class="ti-comments"></i>
                                        </div>
                                        Ihr Kunde
                                    </a>
                                </li>
                                <li>
                                    <a href="#important-at-end" data-toggle="tab">
                                        <div class="icon-circle">
                                            <i class="ti-share"></i>
                                        </div>
                                        Notwendiges zum Schluss
                                    </a>
                                </li>
                            </ul>
                        </div>

                        <br><br>

                        <div class="tab-content">

                            <div class="tab-pane" id="contactinformation">
                                <div class="col-sm-5 col-sm-offset-3">
                                    <h4 class="info-text">Kontaktinformation</h4>
                                </div>

                                <br>
                                <div class="row">
                                    <div class="col-sm-5 col-sm-offset-3">
                                        <div class="form-group">
                                            <label>Ihr Firmenname</label>
                                        </div>
                                        <div class="input-group">
                                            <%= form.text_field :price, class: 'form-control' %>
                                            <span class="input-group-addon"></span>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-sm-5 col-sm-offset-3">
                                        <div class="form-group">
                                            <label>Email adresse</label>
                                        </div>
                                        <div class="input-group">
                                            <%= form.text_field :price, class: 'form-control' %>
                                            <span class="input-group-addon"></span>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-sm-5 col-sm-offset-3">
                                        <div class="form-group">
                                            <label>Telefon</label>
                                        </div>
                                        <div class="input-group">
                                            <%= form.text_field :price, class: 'form-control' %>
                                            <span class="input-group-addon"></span>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <br>
                                    <div class="col-sm-5 col-sm-offset-3">
                                        <div class="form-group">
                                            <input type="checkbox" style="width: 20px; height: 20px;">
                                            <a target="_blank" rel="noopener noreferrer" href="/pages/termsAndConditions">
                                                Datenrechtschutzlinie </a>
                                            <br>
                                        </div>
                                    </div>
                                </div>

                            </div>


                            <div class="tab-pane" id="location">
                                <h4 class="info-text">Das Wichtigste Zuerst</h4>
                                <br>
                                <div class="row">
                                    <div class="col-sm-6 col-sm-offset-3">
                                        <div class="form-group">
                                            <label>Wohin soll es gehen?</label>
                                            <%= form.select :country,
                                                            options_for_select(['Afghanistan', 'Albania', 'Algeria', 'Andorra', 'Angola', 'Antigua and Barbuda', 'Argentina',
                                                                                'Armenia', 'Australia', 'Austria', 'Azerbaijan', 'Bahamas', 'Bahrain', 'Bangladesh', 'Barbados',
                                                                                'Belarus', 'Belgium', 'Belize', 'Benin', 'Bhutan', 'Bolivia', 'Bosnia and Herzegovina', 'Botswana',
                                                                                'Brazil', 'Brunei', 'Bulgaria', 'Burkina Faso', 'Burundi', 'Cabo Verde', 'Cambodia', 'Cameroon',
                                                                                'Canada', 'Central African Republic (CAR)', 'Chad', 'Chile', 'China', 'Colombia', 'Comoros',
                                                                                'Democratic Republic of the Congo', 'Republic of the Congo', 'Costa Rica', 'Cote d\'Ivoire',
                                                                                'Croatia', 'Cuba', 'Cyprus', 'Czech Republic', 'Denmark', 'Djibouti', 'Dominica', 'Dominican Republic',
                                                                                'Ecuador', 'Egypt', 'El Salvador', 'Equatorial Guinea', 'Eritrea', 'Estonia',
                                                                                'Eswatini (formerly Swaziland)', 'Ethiopia', 'Fiji', 'Finland', 'France', 'Gabon',
                                                                                'Gambia', 'Georgia', 'Germany', 'Ghana', 'Greece', 'Grenada', 'Guatemala', 'Guinea',
                                                                                'Guinea-Bissau', 'Guyana', 'Haiti', 'Honduras', 'Hungary', 'Iceland', 'India', 'Indonesia',
                                                                                'Iran', 'Iraq', 'Ireland', 'Israel', 'Italy', 'Jamaica', 'Japan', 'Jordan', 'Kazakhstan',
                                                                                'Kenya', 'Kiribati', 'Kosovo', 'Kuwait', 'Kyrgyzstan', 'Laos', 'Latvia', 'Lebanon', 'Lesotho',
                                                                                'Liberia', 'Libya', 'Liechtenstein', 'Lithuania', 'Luxembourg', 'Macedonia (FYROM)', 'Madagascar',
                                                                                'Malawi', 'Malaysia', 'Maldives', 'Mali', 'Malta', 'Marshall Islands', 'Mauritania', 'Mauritius',
                                                                                'Mexico', 'Micronesia', 'Moldova', 'Monaco', 'Mongolia', 'Montenegro', 'Morocco', 'Mozambique',
                                                                                'Myanmar (formerly Burma)', 'Namibia', 'Nauru', 'Nepal', 'Netherlands', 'New Zealand', 'Nicaragua',
                                                                                'Niger', 'Nigeria', 'North Korea', 'Norway', 'Oman', 'Pakistan', 'Palau', 'Palestine', 'Panama',
                                                                                'Papua New Guinea', 'Paraguay', 'Peru', 'Philippines', 'Poland', 'Portugal', 'Qatar', 'Romania',
                                                                                'Russia', 'Rwanda', 'Saint Kitts and Nevis', 'Saint Lucia', 'Saint Vincent and the Grenadines',
                                                                                'Samoa', 'San Marino', 'Sao Tome and Principe', 'Saudi Arabia', 'Senegal', 'Serbia', 'Seychelles',
                                                                                'Sierra Leone', 'Singapore', 'Slovakia', 'Slovenia', 'Solomon Islands', 'Somalia', 'South Africa',
                                                                                'South Korea', 'South Sudan', 'Spain', 'Sri Lanka', 'Sudan', 'Suriname',
                                                                                'Swaziland (renamed to Eswatini)', 'Sweden', 'Switzerland', 'Syria', 'Taiwan',
                                                                                'Tajikistan', 'Tanzania', 'Thailand', 'Timor-Leste', 'Togo', 'Tonga', 'Trinidad and Tobago',
                                                                                'Tunisia', 'Turkey', 'Turkmenistan', 'Tuvalu', 'Uganda', 'Ukraine', 'United Arab Emirates (UAE)',
                                                                                'United Kingdom (UK)', 'United States of America (USA)', 'Uruguay', 'Uzbekistan', 'Vanuatu',
                                                                                'Vatican City (Holy See)', 'Venezuela', 'Vietnam', 'Yemen', 'Zambia', 'Zimbabwe'],
                                                                               listing.accommodates ? listing.accommodates : nil), {}, id: 'exportercountry', class: 'form-control' %>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-sm-6 col-sm-offset-3">
                                        <div class="form-group">
                                            <label>Handelt es sich bei Ihrer Ware um
                                                Investitionsgüter?</label>
                                            <div class="input-group">
                                                <%= form.check_box :is_investment_good, id: "investmentGood", :style => "width: 20px; height: 20px;" %>
                                                Ja
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div id="showHideDivTotalFinancingRequired">
                                    <div class="row">
                                        <div class="col-sm-6 col-sm-offset-3">
                                            <div class="form-group">
                                                <label>Wie hoch ist der Auftragswert in Euro?</label>
                                                <div class="input-group">
                                                    <%= form.text_field :contract_value, class: 'form-control', placeholder: 'Auftragswert' %>
                                                    <span class="input-group-addon">€</span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div id="showHideDivPaymentGoal">

                                    <div class="row">
                                        <div class="col-sm-6 col-sm-offset-3">
                                            <div class="form-group">
                                                <label> Welches Zahlungsziel gewähren Sie Ihrem Kunden? </label>
                                                <%= form.select :payment_deadline, options_for_select(['3 Months', '6 Months', '9 Months', '12 Months', '15 Months', '18 Months', '21 Months', '24 Months', '27 Months', '30 Months', '33 Months', '36 Months'], listing.is_investment_good ? listing.is_investment_good : nil), {}, class: 'form-control' %>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div id="showHideDivDeliveryGoal">
                                    <div class="row">
                                        <div class="col-sm-6 col-sm-offset-3">
                                            <div class="form-group">
                                                <label>An wen liefern Sie?</label>

                                                <div class="row">

                                                    <div class="col-sm-6">
                                                        <div class="choice <%= 'active' if listing.private_sector == 'on' %>" name = "privatesector" data-toggle="wizard-checkbox" onclick="privateSector()">
                                                            <%= check_box_tag "listing[private_sector]", listing.private_sector ? listing.private_sector : nil %>

                                                            <div class="card card-checkboxes card-hover-effect">
                                                                <i class="ti-home"></i>
                                                                <p>Privater Sektor</p>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="col-sm-6">
                                                        <div class="choice <%= 'active' if listing.public_sector == 'on' %>" name = "publicsector" data-toggle="wizard-checkbox" onclick="publicSector()">
                                                            <%= check_box_tag "listing[public_sector]", listing.public_sector ? listing.public_sector : nil %>

                                                            <div class="card card-checkboxes card-hover-effect">
                                                                <i class="ti-home"></i>
                                                                <p>Öffentlicher Sektor</p>
                                                            </div>
                                                        </div>
                                                    </div>

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>


                                <div id="showHideDivResultMessage" style="display: none">

                                    <div class="row">
                                        <div class="row">
                                            <div class="col-md-9 col-md-offset-2">
                                                <p>
                                                    Ihr Geschäft eignet sich grundsätzlich und vorbehaltlich einer
                                                    tieferen Prüfung des Sachverhalts und der Bonität Ihres
                                                    ausländischen Bestellers bzw. Garantien für eine Hermesdeckung. Nach
                                                    Vorliegen aller relevanten Informationen eine Entscheidung innerhalb
                                                    von durchschnittlich 2 Wochen möglich. Im Rahmen Ihrer weiteren
                                                    Eingaben wird fortwährend geprüft, ob Ihr Geschäft weiterhin für
                                                    eine Hermesdeckung geeignet ist – am Ende können Sie jedoch selbst
                                                    entscheiden, welche Absicherungsvariante Sie wählen. Sollte sich im
                                                    Rahmen der Einzelfallprüfung des Antrags zeigen, dass der digitalen
                                                    Antrag für eine Hermesdeckung nicht zulässig ist, ist weiterhin der
                                                    Zugang zur klassischen Lieferantenkreditdeckung durch Euler Hermes
                                                    möglich.
                                                </p>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div id="showHideDivResultMessageFail" style="display: none">

                                    <div class="row">
                                        <div class="row">
                                            <div class="col-md-9 col-md-offset-2">
                                                <p>
                                                    hr Geschäft birgt Besonderheiten, die eine Absicherung erschweren
                                                    könnten: <br>

                                                    Deckungsmöglichkeiten bei Bestellern des öffentlichen Sektors,
                                                    soweit diese privatwirtschaftlich organisiert sind und ohne
                                                    Rückgriff auf öffentliche Mittel wirtschaften. <br>

                                                    Gern möchten wir Ihnen die besonderen Gegebenheiten in einem
                                                    persönlichen Gespräch erläutern und die Struktur Ihres Geschäfts
                                                    besprechen. Bitte kontaktieren Sie uns über den untenstehenden
                                                    Kontaktbutton oder rufen Sie uns unter +49 (0) 15751174449 an.
                                                </p>
                                            </div>
                                        </div>
                                    </div>
                                </div>


                            </div>
                            <div class="tab-pane" id="type">
                                <h4 class="info-text">Ihre Risken</h4>
                                <h5 class="info-text">Bitte geben Sie an, gegen welche Risiken Sie sich absichern
                                    möchten </h5>

                                <div class="row">
                                    <div class="col-sm-10 col-sm-offset-1">
                                        <div class="col-sm-4 col-sm-offset-2">

                                            <div class="choice <%= 'active' if listing.risk_supplier_credit_cover == 'on' %>" data-toggle="wizard-checkbox">
                                                <%= check_box_tag "listing[risk_supplier_credit_cover]", listing.risk_supplier_credit_cover ? listing.risk_supplier_credit_cover : nil %>

                                                <div class="card card-checkboxes height-adjusted-big card-hover-effect">
                                                    <i class="ti-home"></i>
                                                    <p>Nichtbezahlung </p>
                                                    <p>(bekannt als Lieferantenkreditdeckung)</p>
                                                    <h7 style="color:#F3F2EE;">I</h7>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-sm-4">
                                            <div class="choice <%= 'active' if listing.risk_shipment_risk_cover == 'on' %>" data-toggle="wizard-checkbox">
                                                <%= check_box_tag "listing[risk_shipment_risk_cover]", listing.risk_shipment_risk_cover ? listing.risk_shipment_risk_cover : nil %>
                                                <div class="card card-checkboxes card-hover-effect">
                                                    <i class="ti-package"></i>
                                                    <p>Vertragsstörung während der Fabrikationsphase</p>
                                                    <p>(bekannt als Fabrikationsrisikodeckung)</p>
                                                    <h7 style="color:red;">In Kürze verfügbar</h7>
                                                </div>
                                            </div>
                                        </div>


                                        <div class="row">
                                            <div class="col-sm-4 col-sm-offset-2">
                                                <div class="choice <%= 'active' if listing.risk_contractual_warranty_coverage == 'on' %>" data-toggle="wizard-checkbox" onclick="ziehungVonGarantien()">

                                                    <%= check_box_tag "listing[risk_contractual_warranty_coverage]", listing.risk_contractual_warranty_coverage ? listing.risk_contractual_warranty_coverage : nil %>

                                                    <div class="card card-checkboxes card-hover-effect">
                                                        <i class="ti-layout-accordion-merged"></i>
                                                        <p>Ziehung von Garantien</p>
                                                        <p>(bekannt als Vertragsgarantiedeckung)</p>
                                                        <h7 style="color:red;">In Kürze verfügbar</h7>
                                                    </div>
                                                </div>
                                            </div>


                                            <div id="entlastung" class="col-sm-4" , style="display: none" ;>
                                                <div class="choice <%= 'active' if listing.risk_avaline_guarantee == 'on' %>" data-toggle="wizard-checkbox">
                                                    <%= check_box_tag "listing[risk_avaline_guarantee]", listing.risk_avaline_guarantee ? listing.risk_avaline_guarantee : nil %>
                                                    <div class="card card-checkboxes card-hover-effect">
                                                        <i class="ti-envelope"></i>
                                                        <p>Entlastung Ihrer Avallinie</p>
                                                        <p>(bekannt als Vertragsgarantiedeckung)</p>
                                                        <h7 style="color:red;">In Kürze verfügbar</h7>

                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-sm-4 col-sm-offset-2">
                                            <div class="choice <%= 'active' if listing.risk_further_coverage == 'on' %>" data-toggle="wizard-checkbox">
                                                <%= check_box_tag "listing[risk_further_coverage]", listing.risk_further_coverage ? listing.risk_further_coverage : nil %>

                                                <div class="card card-checkboxes card-hover-effect">
                                                    <i class="ti-layout-grid4"></i>
                                                    <p>Weitere Deckungen</p>
                                                    <h7 style="color:red;">In Kürze verfügbar</h7>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="tab-pane" id="facilities">

                                <h4 class="info-text">Ihr Geschäft</h4>

                                <br>
                                <div class="row">
                                    <div class="col-sm-5 col-sm-offset-1">
                                        <div class="form-group">
                                            <p><b>Bitte beschreiben Sie Ihre Produkte und Leistungen</b></p>
                                            <div class="input-group">
                                                <%= form.text_area :describe_export_business, class: 'form-control' %>
                                                <span class="input-group-addon"></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-8 col-sm-offset-1">
                                        <p><b>Bitte helfen Sie uns nun, das Vorhaben gut zu verstehen</b></p>
                                        <div class="form-group">
                                            <p>Wofür benötigt Ihr Kunde die Maschinen/Güter/Anlagen bzw. die
                                                Dienstleistung? Handelt es sich bspw. um eine Ersatz- oder um eine
                                                Neuinvestition? Wenn Sie Leistungen (Montage, Schulungen vor Ort, etc.)
                                                erbringen, können Sie diese hier ebenfalls beschreiben.</p>
                                            <div class="input-group">
                                                <%= form.text_area :reason_for_buying_good, class: 'form-control' %>
                                                <span class="input-group-addon"></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-sm-8 col-sm-offset-1">
                                        <div class="form-group">
                                            <p><b>Wir erbringen auch Leistungen (Montage, Schulungen vor Ort,
                                                etc.)?</b></p>
                                            <%= form.check_box :also_provide_service_training, :style => "width: 20px; height: 20px;" %>
                                        </div>
                                    </div>

                                </div>
                                <div class="row">
                                    <div class="col-sm-8 col-sm-offset-1">
                                        <div class="form-group">
                                            <p><b>Bitte ordnen Sie die von Ihnen zu liefernden Waren/Güter/Anlagen bzw.
                                                Dienstleistungen Ihrer Branche und der passenden Warenart zu.</b></p>
                                            <%= form.select :product_branch, options_for_select(['Bergbau, inkl. Verarbeitung', 'Erdöl- und Erdgasförderung inkl. Verarbeitung',
                                                                                                 'Chemie', 'Energie', 'Infrastruktur', 'Papier- , Holz-, Leder- und Textilindustrie',
                                                                                                 'Agrarsektor und Nahrungsmittelindustrie', 'Verarbeitende Industrie', 'Umwelttechnik',
                                                                                                 'Dienstleistungen'], listing.breakfast ? listing.breakfast : nil), {}, class: 'form-control' %>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-sm-8 col-sm-offset-1">
                                        <div class="form-group">
                                            <p><b>Bitte beschreiben Sie Ihre Industrie genauer</b></p>
                                            <div class="input-group">
                                                <%= form.text_area :explain_product_branch, class: 'form-control', :input_html => {:rows => 20} %>
                                                <span class="input-group-addon"></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div id="workwithothercompanies">

                                    <div class="row">
                                        <div class="col-sm-8 col-sm-offset-1">
                                            <div class="form-group">
                                                <p><b>Liefern/Leisten Sie im Rahmen eines Gesamtprojekts zusammen mit
                                                    anderen Unternehmen? </b></p>

                                                <div class="col-md-4 col-md-offset-1">
                                                    <div class="choice <%= 'active' if listing.part_of_big_project_yes == 'on' %>" name="deliverwithothercompanies_yes" data-toggle="wizard-checkbox" onclick="deliverWithOtherCompanies()">

                                                        <%= check_box_tag "listing[part_of_big_project_yes]", listing.part_of_big_project_yes ? listing.part_of_big_project_yes : nil %>

                                                        <div class="card circle card-hover-effect">
                                                            <i class="ti-check"></i>
                                                        </div>
                                                    </div>
                                                </div>


                                                <div class="col-md-4 col-md-offset-1">

                                                    <div class="choice <%= 'active' if listing.part_of_big_project_no == 'on' %>" name="deliverwithothercompanies_no" data-toggle="wizard-checkbox" onclick="notDeliverWithOtherCompanies()">

                                                        <%= check_box_tag "listing[part_of_big_project_no]", listing.part_of_big_project_no ? listing.part_of_big_project_no : nil %>

                                                        <div class="card circle card-hover-effect">
                                                            <i class="ti-close"></i>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div id="showHideDivJaWorkOtherCompanies" style="display: none;background-color: #f7f7f7;">

                                    <div class="row">
                                        <div class="col-sm-8 col-sm-offset-1">
                                            <br><br>
                                            <div class="form-group">
                                                <p>Bitte beschreiben Sie das Gesamtprojekt detaillierter</p>
                                                <div class="input-group">
                                                    <%= form.text_area :explain_complete_project, class: 'form-control' %>
                                                    <span class="input-group-addon"></span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-sm-8 col-sm-offset-1">
                                            <div class="form-group">
                                                <p>Wir tragen die Gesamtverantwortung für das Projekt</p>
                                                <div class="row">
                                                    <div class="col-md-4 col-md-offset-2">
                                                        <%= form.check_box :overall_responsibility_project_taken, id: "investmentGood", :style => "width: 20px; height: 20px;" %>
                                                        Ja
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-sm-8 col-sm-offset-1">
                                            <div class="form-group">
                                                <div class="row">
                                                    <p>Wie hoch ist das Gesamtprojektvolumen?</p>
                                                    <div class="col-sm-5 col-sm-offset-1">
                                                        <%= form.select :overall_project_volume_currency, options_for_select(["Euro", "Dollar"]), {}, class: 'form-control', prompt: "Currency..." %>
                                                    </div>
                                                    <div class="col-sm-5">
                                                        <%= form.text_field :overall_project_volume, class: "form-control" %>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
