<!-- step-5 start -->
<div class="step-5 tab">
    <!--   Form Navigation Section   -->
    <div class="row form-nav-top">
        <div class="col s12 m4">
            <div class="form-nav">
                <p><span> 80% Fertig</span></p>
            </div>
        </div>
        <div class="col s12 m4">
            <div class="form-nav">
                <h2>Abschluss</h2>
            </div>
        </div>
        <div class="col s12 m4">
            <div class="form-nav right">
                <!--                                <a href="#">Entwurf speichern</a>-->
                <input class="btn handex-btn z-depth-0" type="submit" name="draft" value="Entwurf speichern">
                <%= form.submit class: 'btn handex-btn z-depth-0', value: t('application_submit') %>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col s12">

            <div class="form-content">
                <div class="row">
                    <div class="col background_toggle has_previously_used_hermes_cover">
                        <div class="input-field col s12 m12">
                            <div class="input-field col s12 m12 nopadding">
                                <%= form.label :has_previously_used_hermes_cover, t('has_previously_used_hermes_cover') %>
                                <p>
                                    <label>
                                        <%= check_box_tag "export_application[has_previously_used_hermes_cover]", @export_application.has_previously_used_hermes_cover ? @export_application.has_previously_used_hermes_cover : nil, false, :onclick => "previouslyHermesInsuranceUsed();", id: "export_application_has_previously_used_hermes_cover" %>
                                        <span class="yes yes-no-btns  <%= 'active-btn' if (@export_application.has_previously_used_hermes_cover.present? && @export_application.has_previously_used_hermes_cover) %>"><%= t :yes_button %></span>
                                    </label>

                                    <label onclick="noPreviouslyHermesInsuranceUsed()">
                                        <span  class="no yes-no-btns  <%= 'active-btn' if (@export_application.has_previously_used_hermes_cover.present? && !@export_application.has_previously_used_hermes_cover) %>"><%= t :no_button %></span>
                                    </label>
                                </p>
                            </div>
                        </div>

                        <div id="newAtHermesDeckung" style="display: none">
                            <div class="row">
                                <br>
                                <div class="col m10 offset-m1">
                                    <p> <%= t('new_to_hermes_cover') %></p>
                                </div>

                                <div class="col m10 offset-m1">
                                    <p><i><%= t('years_trading_without_hermes_insurance') %></i></p>
                                    <div> <%= form.text_field :years_trading_without_hermes_insurance, class: "form-control" %> </div>
                                </div>

                                <div class="col m10 offset-m1">
                                    <p><i><%= t('experience_with_importer_country') %></i></p>
                                    <%= form.text_field :experience_with_importer_country, class: "form-control" %>
                                </div>

                                <div class="col m10 offset-m1 ">
                                    <p><i> <%= t('adequate_claims_management') %></i></p>
                                    <%= form.text_field :adequate_claims_management, class: "form-control" %>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="input-field col s12 m12">
                        <label> <%= t('information_for_business_examination') %> </label>
                        <p> <%= t('information_for_business_examination_description') %> </p>
                    </div>


                    <div class="input-field col s12 m6 ">
                        <%= form.label :self_disclosure, t('self_disclosure') %>
                        <div class="file-field input-field">
                            <div class="btn handex-btn z-depth-0">
                                <span> <%= t('browse') %></span>
                                <%= form.file_field :self_disclosure %>
                                <input type="file" data-height="300"/>
                            </div>

                            <div class="file-path-wrapper">
                                <input class="file-path validate" placeholder="Upload file"
                                       type="text"/>
                            </div>
                        </div>
                    </div>

                    <div class="input-field col s12 m6 ">
                        <%= form.label :annual_financial_statement_importer, t('annual_financial_statement_importer') %>
                        <div class="file-field input-field">
                            <div class="btn handex-btn z-depth-0">
                                <span><%= t('browse') %></span>
                                <%= form.file_field :annual_financial_statement_importer %>
                                <input type="file" data-height="300"/>
                            </div>

                            <div class="file-path-wrapper">
                                <input class="file-path validate" type="text"
                                       placeholder="Upload file"/>
                            </div>
                        </div>
                    </div>

                    <div class="input-field col s12 m6 ">
                        <%= form.label :environment_social_audit, t('environment_social_audit') %>
                        <div class="file-field input-field">

                            <div class="btn handex-btn z-depth-0">
                                <span><%= t('browse') %></span>
                                <%= form.file_field :environment_social_audit %>
                                <input type="file" data-height="300"/>
                            </div>

                            <div class="file-path-wrapper">
                                <input class="file-path validate" type="text"
                                       placeholder="Upload file"/>
                            </div>
                        </div>
                    </div>

                    <div class="input-field col s12 m6 ">
                        <%= form.label :project_memorandum_exporter_business, t('project_memorandum_exporter_business') %>
                        <div class="file-field input-field">
                            <div class="btn handex-btn z-depth-0">
                                <span><%= t('browse') %></span>
                                <%= form.file_field :project_memorandum_exporter_business %>
                                <input type="file" data-height="300"/>
                            </div>

                            <div class="file-path-wrapper">
                                <input class="file-path validate" type="text"
                                       placeholder="Upload file"/>
                            </div>
                        </div>
                    </div>


                    <div class="input-field col s12 m6 ">
                        <%= form.label :credit_bureaus_importer, t('credit_bureaus_importer') %>
                        <div class="file-field input-field">
                            <div class="btn handex-btn z-depth-0">
                                <span><%= t('browse') %></span>
                                <%= form.file_field :credit_bureaus_importer %>
                            </div>

                            <div class="file-path-wrapper">
                                <input class="file-path validate" type="text"
                                       placeholder="Upload file"/>
                            </div>
                        </div>
                    </div>

                    <div class="input-field col s12 m6 ">
                        <%= form.label :additional_document, t('additional_document') %>
                        <div class="file-field input-field">
                            <div class="btn z-depth-0 handex-btn">
                                <span><%= t('browse') %></span>
                                <%= form.file_field :additional_document %>
                            </div>

                            <div class="file-path-wrapper">
                                <input class="file-path validate" type="text"
                                       placeholder="Upload file"/>
                            </div>
                        </div>
                    </div>

                </div>

                <div class="input-field col s12 m12">
                    <label> <%= t('corruption_prevention_statement') %></label>
                </div>
                <div class="input-field col s12 greybox">

                    <div class="input-field col s12 m12">
                        <label> <%= t('compliance_heading') %> </label>
                        <p> <%= t('compliance_statement') %></p>
                    </div>

                    <div class="input-field col s12 m12">
                        <label><%= t('obligation_heading') %></label>
                        <p> <%= t('obligation_statement') %> </p>
                    </div>

                    <div class="input-field col s12 m12">
                        <label><%= t('criminal_information_heading') %> </label>
                        <p> <%= t('criminal_information_statement') %> </p>
                    </div>

                    <div class="input-field col s12 m12">
                        <ul>
                            <li> * <%= t('state_court') %></li>
                            <li> * <%= t('court_case_5_years') %> </li>
                            <li> * <%= t('sanction_by_court') %> </li>
                            <li> * <%= t('subjected_to_public_prosecutor') %> </li>
                            <li> * <%= t('listed_on_blacklist') %> </li>
                        </ul>
                    </div>

                    <div class="input-field col s12 m12">
                        <div class="input-field col s12 m12 nopadding">
                            <p>
                                <label>
                                    <%= check_box_tag "export_application[does_corruption_declaration_apply]", @export_application.does_corruption_declaration_apply ? @export_application.does_corruption_declaration_apply : nil, false, :onclick => "corruptionDeclaration();", id: "export_application_does_corruption_declaration_apply" %>
                                    <span class="yes yes-no-btns <%= 'active-btn' if (@export_application.does_corruption_declaration_apply.present? && @export_application.does_corruption_declaration_apply) %>">Trifft zu</span>
                                </label>

                                <label onclick="noCorruptionDeclaration()">
                                    <span  class="no yes-no-btns <%= 'active-btn' if (@export_application.does_corruption_declaration_apply.present? && !@export_application.does_corruption_declaration_apply) %>">Trifft nicht zu</span>
                                </label>
                            </p>
                        </div>
                    </div>

                    <div id="corruptionDeclaration" style="display: none">
                        <div class="row">
                            <div class="input-field col s12">
                                <%= form.label :explain_corruption_sanctions, t('explain_corruption_sanctions') %>
                                <%= form.text_area :explain_corruption_sanctions, autofocus: true, class: "validate" %>
                            </div>
                        </div>
                    </div>

                </div>

                <div class="input-field col s12 m12">
                    <label>
                        <input type="checkbox"/>
                        <span class="terms"> <%= t('is_all_information_provided_with_best_of_knowledge') %></span>
                    </label>
                </div>

                <div class="input-field col s12 m12">
                    <label>
                        <input type="checkbox"/>
                        <span class="terms">
                                                <%= t("has_accepted_terms_conditions_one_html",  href: link_to(t('has_accepted_terms_conditions_one_text'), t("has_accepted_terms_conditions_one_href"), target: "_blank", rel: "noopener noreferrer")) %>

                            <%= t("has_accepted_terms_conditions_two_html", href: link_to(t('has_accepted_terms_conditions_two_text'), t("has_accepted_terms_conditions_two_href"), target: "_blank", rel: "noopener noreferrer")) %>
                                            </span>
                    </label>
                </div>


                <div class="input-field col s12 m12">
                    <%#= form.check_box :has_read_privacy_policy, type: "checkbox"%>
                    <label>
                        <input type="checkbox"/>
                        <span class="terms">
                                                <%= t("has_read_privacy_policy_html", href: link_to(t('has_read_privacy_policy_text'), t("has_read_privacy_policy_href"), target: "_blank", rel: "noopener noreferrer")) %>
                                            </span>
                    </label>
                </div>

                <div class="input-field col s12 m12">
                    <%#= form.check_box :has_read_all_instructions, :style => "width: 20px; height: 20px;" %>
                    <label>
                        <input type="checkbox"/>
                        <span class="terms">
                                                <%= t("read_all_instruction_html", href: link_to(t('read_all_instruction_text'), t("read_all_instruction_href"), target: "_blank", rel: "noopener noreferrer")) %>
                                            </span>
                    </label>
                </div>

                <div class="input-field col s12 m12">
                    <input class="btn handex-btn z-depth-0" type="submit" name="submit" value="Antrag Shicken">
                    <%#= form.button :submit, name: "submit", value: "Antrag Shicken", class: "btn handex-btn z-depth-0" %>
                </div>
            </div>
        </div>
    </div>
</div>