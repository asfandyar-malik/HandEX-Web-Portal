<p id="notice"><%= notice %></p>

<h1>Export Applications</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @export_applications.each do |export_application| %>
      <tr>
        <td><%= link_to 'Show', export_application %></td>
        <td><%= link_to 'Edit', edit_export_application_path(export_application) %></td>
        <td><%= link_to 'Destroy', export_application, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Export Application', new_export_application_path %>
