      <!--      <div class='disable_input'>-->

      <%# @export_application.attributes.each do |attr_name, attr_value| %>
      <!--          <div class="work_profile_attribute">-->
      <!--            <%#= attr_name.humanize %> : <%#= attr_value %>-->
      <!--          </div>-->
      <%# end %>

      <!--      </div>-->
      <ul class="collection with-header">
        <!--        <li class="collection-header"><h4>First Names</h4></li>-->
        <% @export_application.attributes.each do |attr_name, attr_value|
          next if attr_name == "id"  %>
          <li class="collection-item">
            <div><%= t(attr_name) %>
              <div class="secondary-content">
                <%= if attr_value == "Bitte auswählen"
                      t('not_selected_yet')
                    elsif !attr_value
                      "Nein"
                      # elsif attr_value == ""
                      #     "kein Wert eingegeben!"
                    elsif attr_value
                    end
                %>
              </div>
            </div>
          </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>


