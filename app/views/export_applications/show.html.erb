<%= render 'shared/navbar' %>
<p id="notice" class="" hidden><%= notice %></p>

<div class="overview-top">
    <div class="row">
      <div class="col s6 ">
        <%= link_to :back do %>
          <i class="material-icons">arrow_back</i>
        <% end %>
      </div>
      <div class="col s6">
        <div class="valign-wrapper">
          <i class="material-icons">timer</i>
          <div>
            <strong><%= t('application_sent') %></strong>
          </div>
        </div>
      </div>
    </div>
</div>

<div class="overview-header">
  <div class="container center">
    <div class="col s12 m12">
      <ul>
        <li><%= t('importer') %><div></div></li>
        <li><%= t('product') %><div></div></li>
        <li><%= t('contract_value') %><div></div></li>
        <li><%=  t('application_sent_at')%><div></div></li>
      </ul>
    </div>
  </div>
</div>

<div class="handex-form-container">
  <div class="container">
    <div class="section">

      <ul class="collection with-header">
        <% @export_application.attributes.each do |attr_name, attr_value|
          next if attr_name == "id"  %>
          <li class="collection-item">
            <div><%= t(attr_name) %>
              <div class="secondary-content">
                <%= if attr_value == "Bitte auswÃ¤hlen"
                      t('not_selected_yet')
                    elsif !attr_value
                      t('no')
                    elsif attr_value
                      t('yes')
                    end
                %>
              </div>
            </div>
          </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>

<script>
    $('.disable_input :input').prop('disabled', true);
</script>