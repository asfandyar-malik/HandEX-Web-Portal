<tr class="nested-fields">

  <td>
    <label for="export_application_delivery_start" class="active">Weitere Lieferung</label>
    <br>
    <label for="export_application_delivery_start" class="active">Lieferbeginn</label>
    <div>
      <%= f.text_field :payment_relevant_information, id: "start_date", autofocus: true, class: "datepicker", placeholder: "dd/mm/yyyy" %>
    </div>
  </td>

  <td>
    <br>
    <label for="export_application_delivery_start" class="active">Lieferende</label>
    <%= f.text_field :payment_relevant_milestone, id: "end_date", autofocus: true, class: "datepicker", placeholder: "dd/mm/yyyy" %>
  </td>

  <td>
    <br>
    <%= link_to_remove_association t('delete'), f, class: 'btn btn-primary btn-xs right remove_fields_with_label' %>
  </td>

</tr>