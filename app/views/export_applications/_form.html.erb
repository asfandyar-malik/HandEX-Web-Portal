<script type="text/javascript">
    var curremt_tab = 1;

    $(document).ready(function(){
        $('.datepicker').datepicker();
    });

    $(".yes-no-btns").click(function (e) {
        $(this).toggleClass("active-btn");
        if($(this).hasClass("yes")){
            $(this).closest(".input-field").find(".hide-section").toggle();
            $(".business-cert-no").show();
            $(".business-cert-goods").hide();
            $(this).closest('p').find('.no.yes-no-btns').removeClass("active-btn");
        }else{
            $(this).closest(".input-field").find(".hide-section").hide();
            $(this).closest('p').find('.yes.yes-no-btns').removeClass("active-btn");
        }
    });

    $('.parts-goods').click(function (e) {
        $(".business-cert-no").hide();
        $(".business-cert-goods").show();
    });

    $(".financing-yes-no-btns").click(function (e) {
        $(this).closest(".input-field").find(".hide-section").hide();
        if($(this).hasClass("yes")){
            $(this).closest(".input-field").find(".financing-agreed-yes").show();
        }else{
            $(this).closest(".input-field").find(".financing-agreed-no").show();
        }
    });

    $(".financing-agreed .financing-payment-structure").click(function (e) {
        var agreed_index = $(this).attr('agreed-index');
        $(this).closest('.input-field').find('.main-content-'+agreed_index).toggle();
        for (var i = 1; i <= 3 ; i++) {
            if(agreed_index != i){
                $(this).closest('.input-field').find('.main-content-'+i).hide();
            }
        }
    })

    function changeTabs(curremt_tab_index) {
        $('.tab').hide();
        $('.step-'+curremt_tab_index).show();
    }

    $('.progressbar li, .next').click(function function_name(argument) {
        /*console.log($(this).attr('tab_index'));*/
        changeTabs($(this).attr('tab_index'));
        $('.progressbar li').removeClass('active');
        $(this).addClass('active');

    });

</script>